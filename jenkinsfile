pipeline {
    agent any
  
    stages {
        stage('Verify tooling') {
        def dockerHome = tool 'myDocker'
        env.PATH = "${dockerHome}/bin:${env.PATH}"
            steps {
                script {
                    sh 'docker info'
                    sh 'docker version'
                    sh 'docker-compose version'
                }
            }
        }
    }
}
